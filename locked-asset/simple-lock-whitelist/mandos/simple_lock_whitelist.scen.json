{
    "name": "Simple lock whitelist",
    "comment": "",
    "steps": [
        {
            "step": "setState",
            "accounts": {
                "address:owner": {
                    "nonce": "0",
                    "balance": "0",
                    "esdt": {
                        "str:ASHEGLD-1234": {
                            "balance": "1000000000000000000"
                        },
                        "str:LKLP-1234": {
                            "nonce": "1",
                            "balance": "1000000000000000000",
                            "attributes": "nested:str:ASHEGLD-1234|nested:str:ASH-1234|u64:0|nested:str:WEGLD-1234|u64:0",
