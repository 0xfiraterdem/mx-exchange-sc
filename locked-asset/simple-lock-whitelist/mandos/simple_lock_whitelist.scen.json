                    }
                ],
                "function": "enterFarm",
                "gasLimit": "25,000,000"
            },
            "expect": {
                "status": "0",
                "out": [
                    "1",
                    "0x020000000d41534845474c44462d313233340000000000000001000000080de0b6b3a7640000"
                ]
            }
        },
        {
            "step": "scCall",
            "txId": "enter-farm-locked-token-tx",
            "tx": {
                "from": "address:owner",
                "to": "sc:simple_lock",
                "egldValue": "0",
                "esdtValue": [
                    {
                        "tokenIdentifier": "str:LKLP-1234",
                        "value": "1000000000000000000",
                        "nonce": "1"
                    }
                ],
                "function": "enterFarmLockedToken",
                "gasLimit": "25,000,000"
