                        "str:reward_token_id": "str:MEX-1234",
                        "str:farm_token_id": "str:ASHEGLDF-1234",
                        "str:division_safety_constant": "1000000000000"
                    },
                    "owner": "address:owner"
                },
                "sc:simple_lock": {
                    "nonce": "0",
                    "code": "file:../output/simple-lock-whitelist.wasm",
                    "owner": "address:owner",
                    "esdt": {
                        "str:LKASH-1234": {
                            "roles": [
                                "ESDTRoleNFTCreate",
                                "ESDTRoleNFTAddQuantity",
                                "ESDTRoleNFTBurn"
                            ]
                        },
                        "str:LKLP-1234": {
                            "roles": [
                                "ESDTRoleNFTCreate",
                                "ESDTRoleNFTAddQuantity",
                                "ESDTRoleNFTBurn"
                            ]
                        },
                        "str:LKFARM-1234": {
                            "roles": [
                                "ESDTRoleNFTCreate",
                                "ESDTRoleNFTAddQuantity",
                                "ESDTRoleNFTBurn"
